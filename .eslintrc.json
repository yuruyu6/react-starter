{
  "extends": [
    "plugin:perfectionist/recommended-natural",
    "plugin:@typescript-eslint/recommended",
    "eslint:recommended",
    "next"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "no-undef": "off",
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": [
      "warn",
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_"
      }
    ]
  },
  "root": true
}